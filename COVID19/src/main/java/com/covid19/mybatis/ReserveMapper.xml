<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.covid19.mybatis.ReserveMapper">
<select id="getAllReservation" parameterType="hashMap"
		resultType="ReserveBean">
		SELECT * FROM (SELECT ROWNUM NUM, B.* FROM (SELECT * FROM RESERVATION ORDER BY SYSDATE DESC) B) WHERE NUM <![CDATA[>=]]> #{start} AND NUM <![CDATA[<=]]> #{end}
</select>
<select id="getTotal" resultType="Integer">
		SELECT COUNT(*) AS TOTAL FROM RESERVATION
</select>
<select id="getSelectOneReserve" parameterType="Integer" resultType="ReserveBean">
		SELECT * FROM RESERVATION WHERE NO = #{no}
</select>
<insert id="insertReserve" parameterType="ReserveBean">
		INSERT INTO RESERVATION VALUES (RESERVATION_SEQ.NEXTVAL,
									#{gubun},
									#{vaccine},
									#{hospitalAdd},
									#{rsDate},
									SYSDATE
									)
	</insert>
	
</mapper>