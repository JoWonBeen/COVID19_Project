 CREATE TABLE "COVID19"."RESERVATION" 
   (	"NO" NUMBER, 
	"GUBUN" VARCHAR2(500 BYTE), 
	"VACCINE" VARCHAR2(500 BYTE), 
	"HOSPITALADD" VARCHAR2(500 BYTE), 
	"RSDATE" VARCHAR2(300 BYTE), 
	"REGDATE" DATE,
	"MEMBERID VARCHAR2(300 BYTE)"
   );
   
   
   CREATE SEQUENCE  "COVID19"."RESERVATION_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE ;


